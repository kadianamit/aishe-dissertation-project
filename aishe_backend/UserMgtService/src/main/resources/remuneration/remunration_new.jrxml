	<parameter name="surveyYear" class="java.lang.Integer"/>
	<parameter name="aisheCode" class="java.lang.String"/>
	<parameter name="fromDate" class="java.util.Date"/>
	<parameter name="toDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="institutionType" class="java.lang.String"/>
	<parameter name="stateCode" class="java.lang.String"/>
	<parameter name="districtCode" class="java.lang.String"/>
	<parameter name="categoryType" class="java.lang.String"/>
	<parameter name="universityId" class="java.lang.String"/>
	<parameter name="statusId" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[WITH remuneration_report (institution_type ,user_id, survey_year,
                           account_holder_name, account_number, ifsc_code, updated_on, pan, aishe_code, bank_address, bank_name, pin_code,
                           state_code ,state_name,district_code ,district_name,institution_name,type_id ,type ,affilating_university_id,affilating_university_name,nodal_officer_name,status_id,
						  status_name,remarks,amount,affiliated, submitted,new_university) as (

						   SELECT 'College Institution' as institution_type ,user_id, ua.survey_year, account_holder_name, account_number, ifsc_code, updated_on, pan, ua.aishe_code, bank_address, bank_name, ua.pin_code,
                           s.st_code as state_code ,s.name as state_name,rd.dist_code as district_code ,rd.name as district_name,ru.name as institution_name,rut.id as type_id ,rut.type ,ru1.id as affilating_university_id,ru1.name as affilating_university_name
                           ,ids.officer_name,ua.status_id,rrs.status as status_name,ua.remarks,ua.amount,0,0,'NA'
							   FROM public.user_account ua
							   left join college ru on 'C-'||ru.id=ua.aishe_code and ru.survey_year=ua.survey_year
                           left join ref_university ru1 on ru1.id=ru.university_id and ru1.survey_year=ua.survey_year
                           left join webdcf.person_details_survey ids on (ids.inst_type||'-'||ids.aishe_code =ua.aishe_code and ids.survey_year= ua.survey_year and ids.officer_type='NO' )
                           left join ref_state s on s.st_code=ru.state_code left join ref_district rd on rd.dist_code=ru.district_code
						   left join ref_remuneration_status rrs on rrs.id=ua.status_id
                           left join ref_university_college_type rut on rut.id=ru.type_id
                           where ua.survey_year=$P{surveyYear}
						   and (ua.aishe_code=$P{aisheCode} OR 'ALL'=$P{aisheCode})
                           AND (CAST(ua.updated_on AS date) >= $P{fromDate} )
						   AND (CAST(ua.updated_on AS date) <= $P{toDate} )
                           AND ('College Institution'=$P{institutionType} or 'ALL'=$P{institutionType})
						   and (ru.state_code=$P{stateCode} or 'ALL'=$P{stateCode})
                           and (ru.district_code=$P{districtCode} or 'ALL'=$P{districtCode})
						   and (ru.type_id=$P{categoryType} or 'ALL'=$P{categoryType})
						   and (ru.university_id=$P{universityId} or 'ALL'=$P{universityId})
							and (ua.status_id=$P{statusId} or 0 =$P{statusId})
                           and ru.id is not null  and ua.aishe_code is not null

                            union all

						   SELECT 'Standalone Institution' as institution_type ,user_id, ua.survey_year, account_holder_name, account_number, ifsc_code, updated_on, pan, ua.aishe_code, bank_address, bank_name, ua.pin_code,
                           s.st_code as state_code ,s.name as state_name,rd.dist_code as district_code ,rd.name as district_name,ru.name as institution_name,cast(rut.id as text) as type_id ,rut.type ,null as affilating_university_id,null as affilating_university_name
                           ,ids.officer_name,ua.status_id,rrs.status as status_name,ua.remarks,ua.amount,0,0,'NA'
							   FROM public.user_account ua left join ref_standalone_institution ru on 'S-'||ru.id=ua.aishe_code and ru.survey_year=ua.survey_year and ru.id is not null  and ua.aishe_code is not null
                           left join webdcf.person_details_survey ids on (ids.inst_type||'-'||ids.aishe_code =ua.aishe_code and ids.survey_year= ua.survey_year and ids.officer_type='NO' )
                           left join ref_state s on s.st_code=ru.statecode
                           left join ref_district rd on rd.dist_code=ru.district_code
						    left join ref_remuneration_status rrs on rrs.id=ua.status_id
                           left join ref_state_body rut on rut.id=ru.statebodyid
                           where ua.survey_year=$P{surveyYear}
                           and (ua.aishe_code=$P{aisheCode} OR 'ALL'=$P{aisheCode})
                           AND (CAST(ua.updated_on AS date) >= $P{fromDate} )
						   AND (CAST(ua.updated_on AS date) <= $P{toDate})
                           AND ('Standalone Institution'=$P{institutionType} or 'ALL'=$P{institutionType})
						   and (ru.statecode=$P{stateCode} or 'ALL'=$P{stateCode})
                           and (ru.district_code=$P{districtCode} or 'ALL'=$P{districtCode})
						   and (CAST(ru.statebodyid AS TEXT)=$P{categoryType} or 'ALL'=$P{categoryType})
							   and (ua.status_id=$P{statusId} or 0 =$P{statusId})
                            and ru.id is not null  and ua.aishe_code is not null
                            union all

                           SELECT 'University' as institution_type ,user_id, ua.survey_year, account_holder_name, account_number, ifsc_code, updated_on, pan, ua.aishe_code, bank_address, bank_name, ua.pin_code,
                           s.st_code as state_code ,s.name as state_name,rd.dist_code as district_code ,rd.name as district_name,ru.name as institution_name,rut.id as type_id ,rut.type ,null as affilating_university_id,null as affilating_university_name
                           ,ids.officer_name,ua.status_id,rrs.status as status_name,ua.remarks,ua.amount,
							   ua.number_of_affiliating_college,ua.number_of_dcf_submitted_college,
							   case when ua.is_this_new_university ='true' then 'Yes' else 'No' end
							   FROM public.user_account ua left join ref_university ru on (ua.aishe_code='U-'||ru.id and ru.survey_year=ua.survey_year)
                           left join webdcf.person_details_survey ids on (ids.inst_type||'-'||ids.aishe_code =ua.aishe_code and ids.survey_year= ua.survey_year and ids.officer_type='NO' )
                           left join ref_state s on s.st_code=ru.statecode left join ref_district rd on rd.dist_code=ru.district_code
						    left join ref_remuneration_status rrs on rrs.id=ua.status_id
                           left join ref_university_type rut on rut.id=ru.type_id
                           where ua.survey_year=$P{surveyYear}  and (ua.aishe_code=$P{aisheCode} OR 'ALL'=$P{aisheCode})
                           AND (CAST(ua.updated_on AS date) >= $P{fromDate})
						   AND (CAST(ua.updated_on AS date) <= $P{toDate} )
                           AND ('University'=$P{institutionType} or 'ALL'=$P{institutionType})
						   and (ru.statecode=$P{stateCode} or 'ALL'=$P{stateCode})
                           and (ru.district_code=$P{districtCode} or 'ALL'=$P{districtCode})
						   and (ru.type_id=$P{categoryType} or 'ALL'=$P{categoryType})
							   and (ua.status_id=$P{statusId} or 0 =$P{statusId})
                                and ru.id is not null  and ua.aishe_code is not null


                           )
                           select * from remuneration_report order by institution_type,aishe_code,updated_on]]>
	</queryString>
