	<variable name="percentage" class="java.lang.Double">
		<variableExpression><![CDATA[($F{affiliated} != null && $F{affiliated}.doubleValue() != 0) ?
    ($F{submitted} != null ? $F{submitted}.doubleValue() / $F{affiliated}.doubleValue() * 100 : 0) :
    null]]></variableExpression>
		<initialValueExpression><![CDATA[java.lang.Integer.valueOf(0)]]></initialValueExpression>
	</variable>
	<variable name="r_amount" class="java.lang.Double">
		<variableExpression><![CDATA[($F{amount} != null && $V{percentage} != null) ?
    ($F{amount}.doubleValue() * $V{percentage}.doubleValue() / 100) :
$F{amount}]]></variableExpression>
		<initialValueExpression><![CDATA[java.lang.Integer.valueOf(0)]]></initialValueExpression>
	</variable>
